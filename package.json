{
  "name": "reacthttp",
  "version": "0.0.5",
  "description": "An universal http module for react",
  "main": "dist/react-http.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "rimraf": "node_modules/.bin/rimraf",
    "test": "node_modules/.bin/mocha --require babel-core/register",
    "prebuild": "npm run rimraf -- lib",
    "build": "node_modules/.bin/webpack",
    "preprod": "npm run rimraf -- dist",
    "prod": "WEBPACK_ENV=prod node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinpee/reacthttp.git"
  },
  "keywords": [
    "react",
    "http",
    "universal"
  ],
  "author": "Tinpee <tinpee.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinpee/react-http/issues"
  },
  "homepage": "https://github.com/tinpee/react-http#readme",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "form-data": "^2.1.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mocha": "^3.1.2",
    "multer": "^1.2.0",
    "should": "^11.1.1",
    "webpack": "^1.13.2"
  }
}
